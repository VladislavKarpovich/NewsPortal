{
    "extends": "airbnb-base",
    "rules": {
        "no-use-before-define": ["warn", { "functions": false }],
        "linebreak-style": ["warn", "windows"],
        "func-names": "off",
        "strict": "error",
        "no-unused-vars": "off",
        "no-return-assign": "off",
        "no-param-reassign": "off",
        "no-underscore-dangle": "off",
        "no-useless-return": "off",
        "eqeqeq": "off",
        "comma-dangle": "off",
        "no-plusplus": "off"
    },
    "globals": {
        "document": false,
        "window": false,
        "XMLHttpRequest": false,
        "requests": false,
        "messageService": false,
        "articleList": false,
        "heyId": false,
        "heyClassName": false,
        "heyQuery": false,
        "queryAll": false
    }    
}